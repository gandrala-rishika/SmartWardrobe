# The URL for your backend API during local development.
# Default for many setups is http://127.0.0.1:8000 or http://localhost:8000.
REACT_APP_DEV_BACKEND_URL=http://127.0.0.1:8000

# The URL for your live production backend API.
# IMPORTANT: Replace this with your actual production URL when deploying.
REACT_APP_PROD_BACKEND_URL=https://your-production-backend-url.com

# The port for the Webpack Dev Server socket (used for Hot Reloading).
# Default for Create React App is 3000.
WDS_SOCKET_PORT=3000

# Set to 'true' to enable visual editing features in the app.
REACT_APP_ENABLE_VISUAL_EDITS=false

# Set to 'true' to enable a development health check endpoint.
ENABLE_HEALTH_CHECK=false